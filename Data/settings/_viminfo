"2	CHAR	0
	data rights;
	set test;
	length _grantee _hlqgrantee _dirname _lagdirname $200 _dirpath $ 2000 _owner _right _hlqright $ 20 _userdir 3;
	i=1;
	Do while (scan(dirpath, i, " /") ^= "");
		_lagdirname = _dirname;
		_dirname = scan(dirpath, i, " /"); put _dirname=;
		_dirpath = trim(_dirpath) !! "/" !! trim(_dirname); put _dirpath=;
		Select;
			When (i=1 and put (_dirname, $owner.) ^= '-') _owner = put (_dirname, $owner.);
			When (_dirname =: "XN") do; _grantee = put (_dirname, $usergrp.); _userdir = 1; put "XN" _grantee= _userdir=; end;
			When (_userdir^=1 and index ("&orgdirs.", trim(_dirname))) do; _grantee = symgetc(_dirname); put "ORGDIR" _grantee; end;
			/* Wg. /rcak/CCDataHotel/Cvar/prod/input/gsi entfaellt _lagdirname = "interface" */
			When (_userdir^=1 and lowcase(_dirname) = "output" /*and _lagdirname = "interface"*/) do; _right = "rx"; put "OUTPUT" _right; end;
			When (_userdir^=1 and lowcase(_dirname) = "input" /*and _lagdirname = "interface"*/) do; _right = "rwx"; put "INPUT" _right; end;
			otherwise;
		End;
		i+1;
	End;
	if _grantee ^= "" and _right = "" then _right = "rx";
	* Rechte auf Basis des  High Level Namens;
	If (put (scan(dirpath, 1, " /"), $hlqrght.) ^= '-') then do;
		_hlqgrantee = scan(put (scan(dirpath, 1, " /"), $hlqrght.), 1);
		_hlqright = scan(put (scan(dirpath, 1, " /"), $hlqrght.), 2);
		put "hlq" _hlqgrantee= _hlqright=;
	end;
	* Verzeichnis-spezifische Rechte haben Vorrang vor Rechten auf Basis des HLQ;
	if index (_grantee, _hlqgrantee) then do;
		_hlqgrantee = '';
		_hlqright = '';
	end;
	run;
